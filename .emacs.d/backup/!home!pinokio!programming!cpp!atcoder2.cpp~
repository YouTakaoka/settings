#include<cstdio>
#include<cstring>

char str[2*100000];

int main(){
  int m;
  m = scanf("%s", str);
  if(m <= 0)
    return 1;
  
  int l = strlen(str);
  int black = 0, ans = 0;
  
  for(int i = 0; i < l; i++){
    if(str[i] == 'B')
      black++;
    else
      ans += black;
  }
  printf("%d\n", ans);
  return 0;
}
